{
    "apiVersion": "1.0.0",
    "swaggerVersion": "1.2",
    "basePath": "_2PATCH_",
    "resourcePath": "/users",
    "produces": [
        "application/json"
    ],
    "authorizations": {},
    "apis": [
        {
            "path": "/users",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get a specific User",
                    "notes": "",
                    "type": "User",
                    "nickname": "getUser",
                    "authorizations": {},
                    "parameters": [
                        {
                            "name": "id",
                            "type": "path",
                            "description": "id of the user to be queried",
                            "required": true,
                            "allowMultiple": false
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 401,
                            "message": "Unauthorized"
                        },
                    ]
                },
            ]
        },
        {
            "path": "/users",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Create a new user",
                    "notes": "",
                    "type": "void",
                    "nickname": "sign_up",
                    "authorizations": {},
                    "parameters": [
                        {
                            "name": "body",
                            "description": "email belongs to the user",
                            "required": true,
                            "type": "UserWithPassword",
                            "paramType": "body"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 409,
                            "message": "User already exists"
                        },
                    ]
                }
            ]
        }
    ],
    "models": {
        "User": {
            "id": "User",
            "name": "User",
            "properties": {
                "email": {
                    "type": "string",
                },
                "gender": {
                    "type": "integer",
                    "format": "int32"
                },
                "loc": {
                    "type": "integer",
                    "format": "int32"
                },
                "bday": {
                    "type": "string",
                    "format": "date-time"
                },
                "dtype": {
                    "type": "string"
                }
            },
            "required": [
                "email", "gender", "loc", "bday"
            ],
            "discriminator": "dtype",
            "subTypes": ["UserWithPassword"]
        },
        "UserWithPassword": {
            "id": "UserWithPassword",
            "name": "UserWithPassword",
            "properties": {
                "password": {
                    "type": "string"
                }
            },
            "required": [
                "password"
            ]
        }
    }
}
